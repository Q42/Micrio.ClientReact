	"dependencies": {
		"@micrio/client": "^5.3.2"
	},
	"devDependencies": {
		"@testing-library/react": "^16.0.0",
		"@types/node": "^22.14.1",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.3.1",
		"react": "^18.3.1",
